<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/exploreFragment">
    <fragment
        android:id="@+id/exploreFragment"
        android:name="abhishek.tripstory.fragments.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment2_to_myTripFragment2"
            app:destination="@id/myTripFragment" />
    </fragment>
    <fragment
        android:id="@+id/myTripFragment"
        android:name="abhishek.tripstory.fragments.MyTripFragment"
        android:label="fragment_my_trip"
        tools:layout="@layout/fragment_my_trip" >
        <action
            android:id="@+id/action_myTripFragment2_to_exploreFragment2"
            app:destination="@id/exploreFragment" />
    </fragment>
</navigation>